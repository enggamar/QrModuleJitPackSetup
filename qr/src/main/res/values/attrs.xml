<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AvatarImageBehavior">
        <attr name="finalXPosition" format="dimension" />
        <attr name="finalYPosition" format="dimension" />
        <attr name="finalHeight" format="dimension" />
        <attr name="finalToolbarHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AvatarTextBehavior">
        <attr name="finalXPositionText" format="dimension" />
        <attr name="finalYPositionText" format="dimension" />
        <attr name="finalHeightText" format="dimension" />
        <attr name="finalToolbarHeightText" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ScannerOverlay">
        <attr name="square_width" format="integer" />
        <attr name="square_height" format="integer" />
        <attr name="line_color" format="color" />
        <attr name="line_width" format="integer" />
        <attr name="line_speed" format="integer" />
    </declare-styleable>

    <declare-styleable name="BarcodeReader">
        <attr name="auto_focus" format="boolean" />
        <attr name="use_flash" format="boolean" />
    </declare-styleable>

    <declare-styleable name="KFCAutoCompleteView">
        <attr name="kfc_auto_text_size" format="dimension" />
        <attr name="kfc_auto_show_label" format="boolean" />
        <attr name="kfc_auto_show_flag" format="boolean" />
        <attr name="kfc_auto_enable" format="boolean" />
        <attr name="kfc_auto_hint" format="string" />
        <attr name="kfc_auto_hint_color" format="integer" />
        <attr name="kfc_auto_text_color" format="integer" />
    </declare-styleable>

    <declare-styleable name="CustomRoundedImageView">
        <attr name="criv_corner_radius" format="dimension" />
        <attr name="criv_corner_radius_top_left" format="dimension" />
        <attr name="criv_corner_radius_top_right" format="dimension" />
        <attr name="criv_corner_radius_bottom_left" format="dimension" />
        <attr name="criv_corner_radius_bottom_right" format="dimension" />
        <attr name="criv_border_width" format="dimension" />
        <attr name="criv_border_color" format="color" />
        <attr name="criv_mutate_background" format="boolean" />
        <attr name="criv_oval" format="boolean" />
        <attr name="android:scaleType" />
        <attr name="criv_tile_mode">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
        <attr name="criv_tile_mode_x">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
        <attr name="criv_tile_mode_y">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
    </declare-styleable>

</resources>